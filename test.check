#test DecimalNoToRomanNo

	int decimal = 10;	
	char *c1;
	c1 = (char *)malloc(30);
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"X");
	decimal = 12;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"XII");
	decimal = 44;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"XLIV");
	decimal = 99;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"XCIX");



















