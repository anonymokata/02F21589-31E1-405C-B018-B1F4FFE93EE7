#test DecimalNoToRomanNo

	int decimal = 1000;	
	char *c1;
	c1 = (char *)malloc(30);
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"M");
	decimal = 1112;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"MCXII");
	decimal = 4444;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"MMMMCDXLIV");
	decimal = 9999;
	c1 = DecimalToRoman(decimal);
	ck_assert_str_eq(c1,"MMMMMMMMMCMXCIX");



















